&lt;th class=&#34;ltx_td ltx_align_left ltx_th ltx_th_row ltx_border_l ltx_border_r ltx_border_t&#34;&gt;3&lt;/th&gt;
&lt;td class=&#34;ltx_td ltx_align_justify ltx_border_r ltx_border_t&#34; style=&#34;width:113.8pt;&#34;&gt;1&lt;/td&gt;
&lt;/tr&gt;
&lt;tr class=&#34;ltx_tr&#34;&gt;
&lt;th class=&#34;ltx_td ltx_align_left ltx_th ltx_th_row ltx_border_l ltx_border_r ltx_border_t&#34;&gt;4&lt;/th&gt;
&lt;td class=&#34;ltx_td ltx_align_justify ltx_border_r ltx_border_t&#34; style=&#34;width:113.8pt;&#34;&gt;&lt;math id=&#34;S1.p11.m3&#34; class=&#34;ltx_Math&#34; alttext=&#34;\geq 1&#34; display=&#34;inline&#34;&gt;&lt;mrow&gt;&lt;mi&gt;&lt;/mi&gt;&lt;mo&gt;≥&lt;/mo&gt;&lt;mn&gt;1&lt;/mn&gt;&lt;/mrow&gt;&lt;/math&gt;&lt;/td&gt;
&lt;/tr&gt;
&lt;tr class=&#34;ltx_tr&#34;&gt;
&lt;th class=&#34;ltx_td ltx_align_left ltx_th ltx_th_row ltx_border_l ltx_border_r ltx_border_t&#34;&gt;5&lt;/th&gt;
&lt;td class=&#34;ltx_td ltx_align_justify ltx_border_r ltx_border_t&#34; style=&#34;width:113.8pt;&#34;&gt;1&lt;/td&gt;
